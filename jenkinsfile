pipeline { 
    agent any 
    stages {
        stage('Build') { 
            steps { 
                parallel(
                    "Parallel stage #1": {
                     echo 'build1'
                },
                    "Parallel stage #1": {
                     echo 'build2'
                })
            }
        }
        stage('Test'){
            steps {
                echo 'test'
            }
        }
        stage('Deploy') {
            steps {
                echo 'deploy'
            }
        }
    }
}